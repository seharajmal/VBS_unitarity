set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model SMEFTsim_U35_MwScheme_UFO-cW_massless
define p = g u c d s u~ c~ d~ s~ 
define j = p
define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate p p > l+ l+ vl vl j j QCD=0 SMHLOOP=0 NP=1 NP^2==2 @ 1
add process p p > l- l- vl~ vl~ j j QCD=0 SMHLOOP=0 NP=1 NP^2==2 @ 2
output VBS_SSWW_cW_bsm -nojpeg
